<h1>Hey there <%= @user.first_name %></h1>
<h2> Here's all your pictures </h2>
<% @user.pictures.each do |p|%>
	<%= link_to image_tag(p.url, size: "200x200"),{:controller => "display", :action => "show", :id => p.id} %>
<% end %>

<% @user.listing.images.each do |i|%>
  <%= image_tag i.pic.url %>
<% end %>

<%= link_to "Back to home", home_main_path%>
<%= link_to "Upload", images_path %>
